{
    "name": "magentron/laravel-scripts",
    "description": "Useful scripts etc. for Laravel",
    "type": "library",
	"keywords": ["laravel"],
	"homepage": "https://github.com/Magentron/laravel-scripts",
	"time": "2018-11-26 16:54:04",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Jeroen Derks",
            "email": "jeroen@derks.it",
			"homepage": "https://www.derks.it/",
			"role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
		"psr-4": {
			"Magentron\\LaravelScripts\\": "src"
		},
		"files": [
			"helpers.php"
		]
	},
	"extra": {
		"laravel": {
			"providers": [
				"Magentron\\LaravelScripts\\Providers\\LaravelScriptsServiceProvider"
			]
		}
	},
    "require-dev": {
        "phpstan/phpstan": "0.12.x-dev",
        "friendsofphp/php-cs-fixer": "9999999-dev",
        "phpmd/phpmd": "9999999-dev",
        "squizlabs/php_codesniffer": "4.0.x-dev",
        "sebastian/phpcpd": "6.0.x-dev",
        "phploc/phploc": "7.0.x-dev"
    }
}
